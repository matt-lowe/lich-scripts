	output.concat "\n"
	output.concat "   #{$clean_lich_char}#{script.name} need\n"
	output.concat "\n"
	output.concat "   #{$clean_lich_char}#{script.name} task alchemy\n"
	output.concat "   #{$clean_lich_char}#{script.name} task potions\n"
